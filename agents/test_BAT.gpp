procedure main(){
    start(playSound("Rickroll"));
    navigateToFrame("frame1");
    increaseHeight();
    navigateToFrame("frame2");
    reduceHeight();
    navigateToFrame("frame3");
    normalPose();
    navigateToFrame("frame4");
}